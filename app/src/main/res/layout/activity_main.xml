<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Logica.MainActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="129dp"
        android:layout_height="106dp"
        android:layout_marginStart="36dp"
        app:layout_constraintBottom_toTopOf="@+id/productButton"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/_370329" />

    <Button
        android:id="@+id/productButton"
        style="@style/Widget.Material3.Button"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="208dp"
        android:onClick="pressProductButton"
        android:text="Ver productos"
        app:layout_constraintEnd_toStartOf="@+id/Elotrobutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Elotrobutton"
        style="@style/Widget.Material3.Button"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="69dp"
        android:onClick="pressMateriasButton"
        android:text="Ver materias primas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/productButton"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="129dp"
        android:layout_height="106dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintBottom_toTopOf="@+id/Elotrobutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/_965919_200" />


</androidx.constraintlayout.widget.ConstraintLayout>